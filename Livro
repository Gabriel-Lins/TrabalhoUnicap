class Livro {
    String titulo;
    String autor;

    public Livro(String titulo, String autor) {
        this.titulo = titulo;
        this.autor = autor;
    }

    public void exibirDados() {
        System.out.println("TÃ­tulo do livro: " + titulo);
        System.out.println("Autor: " + autor);
    }
}
